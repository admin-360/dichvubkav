          "_customHTML": "<section data-bs-version=\"5.1\" class=\"features15 digitalm4_features15\" group=\"Features\" data-bg-video=\"{{bg.type == 'video'&&bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"3\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"0\">\n        <header>Show / Hide</header>\n        <input type=\"checkbox\" title=\"Badge\" name=\"showBadge\" checked>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n        <input type=\"checkbox\" title=\"Content Title\" name=\"showContentTitle\" checked>\n        <input type=\"checkbox\" title=\"Content Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Counter\" name=\"showCounter\">\n        <select title=\"Cards count\" name=\"cardsCount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\" selected>3</option>\n        </select>\n        <select title=\"Rows count\" name=\"rowsCount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\" selected>2</option>\n        </select>\n        <header>Colors</header>\n        <input type=\"color\" title=\"Badge Bg Color\" name=\"badgeBg\" value=\"#848abd\" selected condition=\"showBadge\">\n        <input type=\"checkbox\" title=\"Hover effect\" name=\"hoverEffect\">\n        <input type=\"color\" title=\"Card Hover\" name=\"hoverColor\" value=\"#000000\" condition=\"hoverEffect\">\n        <input type=\"color\" title=\"Card 1 Color\" name=\"cardBg1\" value=\"#d16b86\">\n        <input type=\"color\" title=\"Card 2 Color\" name=\"cardBg2\" value=\"#5ca7db\" condition=\"cardsCount > 1\">\n        <input type=\"color\" title=\"Card 3 Color\" name=\"cardBg3\" value=\"#9ccd7e\" condition=\"cardsCount > 2\">\n        <input type=\"color\" title=\"Card 4 Color\" name=\"cardBg4\" value=\"#848abd\" condition=\"cardsCount > 3\">\n        <input type=\"color\" title=\"Card 5 Color\" name=\"cardBg5\" value=\"#fb9d7e\" condition=\"cardsCount > 4\">\n        <input type=\"color\" title=\"Card 6 Color\" name=\"cardBg6\" value=\"#b97ecd\" condition=\"cardsCount > 5\">\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/background1-5.jpg\" parallax>\n            <input type=\"color\" title=\"Background Color\" value=\"#f8f7f3\" selected>\n            <input type=\"video\" title=\"Background Video\" value=\"http://www.youtube.com/watch?v=uNCr7NdOJgw\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay&&bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\" condition=\"overlay&&bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"container justify-content-center\">\n        <h3 class=\"mbr-fonts-style mbr-section-subtitle align-center mbr-light pt-3\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">OUR PROCESS</h3>\n        <div class=\"badge_wrap align-center\">\n            <h5 class=\"badge mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".badge, .badge_wrap\" mbr-if=\"showBadge\">Nền tảng hỗ trợ Doanh nghiệp chuyển đổi số&nbsp;</h5>\n        </div>\n        <h2 class=\"mbr-fonts-style mbr-section-title align-center\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title,.underline\">Nền tảng để Doanh nghiệp chuyển đổi số thành công</h2>\n        <div class=\"row justify-content-center\">\n            <div class=\"col-sm-12\" mbr-class=\"{'col-lg-6': cardsCount == '2','col-lg-4': cardsCount == '3'}\">\n                <div class=\"icon-main\">\n                    <div class=\"process-icon d-flex flex-row justify-content-center\">\n                        <div class=\"wrapper\">\n                            <span class=\"icon-container\">\n                                <a href=\"https://van.bkav.com/Views/Account/Login.aspx\" target=\"_blank\">\n                                    <span mbr-icon class=\"icon-wrapper icon1 mbr-iconfont mobi-mbri-contact-form mobi-mbri\" data-app-selector=\".mbr-iconfont\" mbr-style=\"{'background-color': cardBg}\"></span>\n                                </a>\n                                <p class=\"step step_1 mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCounter\" data-app-selector=\".icon-main .process-icon .wrapper .icon-container .step_1\">1</p>\n                            </span>\n                        </div>\n                        <div class=\"text-wrap\">\n                            <h1 class=\"subicon-title mbr-fonts-style\" data-app-selector=\".icon-main .process-icon .text-wrap .subicon-title\" mbr-theme-style=\"display-4\" mbr-if=\"showContentTitle\"> \n<div><span style=\"font-size: 27.2px;\"><b><a href=\"https://van.bkav.com/Views/Account/Login.aspx\" class=\"text-primary\" target=\"_blank\">Thuế </a><i><a href=\"https://van.bkav.com/Views/Account/Login.aspx\" class=\"text-primary\" target=\"_blank\">điện </a></i><a href=\"https://van.bkav.com/Views/Account/Login.aspx\" class=\"text-primary\" target=\"_blank\">tử TVAN</a></b></span></div></h1>\n                            <p class=\"subicon-text mbr-fonts-style\" data-app-selector=\".icon-main .process-icon .text-wrap .subicon-text\" mbr-if=\"showText\" mbr-theme-style=\"display-7\">- Nộp tờ khai thuế <br>- Nộp thuế qua mang&nbsp;<br></p>\n                        </div>\n\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-sm-12\" mbr-if=\"cardsCount>1\" mbr-class=\"{'col-lg-6': cardsCount == '2','col-lg-4': cardsCount == '3'}\">\n                <div class=\"icon-main\">\n                    <div class=\"process-icon d-flex flex-row justify-content-center\">\n                        <div class=\"wrapper\">\n                            <span class=\"icon-container\">\n                                <a href=\"index.html\">\n                                    <span mbr-icon class=\"mbr-iconfont icon2 icon-wrapper mbrib-protect\" data-app-selector=\".mbr-iconfont\" mbr-style=\"{'background-color': cardBg}\"></span>\n                                </a>\n                                <p class=\"step step_2 mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCounter\" data-app-selector=\".icon-main .process-icon .wrapper .icon-container .step_2\">\n                                    2</p>\n                            </span>\n                        </div>\n                        <div class=\"text-wrap\">\n                            <h1 class=\"subicon-title mbr-fonts-style\" data-app-selector=\".icon-main .process-icon .text-wrap .subicon-title\" mbr-theme-style=\"display-7\" mbr-if=\"showContentTitle\"><i><a href=\"chukyso.html\" class=\"text-primary\">Hải</a></i><a href=\"chukyso.html\" class=\"text-primary\"> quan điện tử&nbsp;&nbsp;</a></h1>\n                            <p class=\"subicon-text mbr-fonts-style\" data-app-selector=\".icon-main .process-icon .text-wrap .subicon-text\" mbr-if=\"showText\" mbr-theme-style=\"display-7\">Đăng ký CKS để khai hải qua